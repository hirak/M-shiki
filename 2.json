{
    "title": "hiraku set",
    "rules": [
        {
            "description": "単発left_controlでescを入力",
            "type": "basic",
            "from": {
                "key_code": "left_control",
                "modifiers": {
                    "optional": ["any"]
                }
            },
            "to": [
                {
                    "key_code": "left_control",
                    "lazy": true
                }
            ],
            "to_if_alone": [
                { "key_code": "escape" }
            ]
        },
        {
            "description": "英数押しっぱなしでshift",
            "type": "basic",
            "from": {
                "key_code": "lang2",
                "modifiers": {
                    "optional": ["any"]
                }
            },
            "to": [
                {
                    "key_code": "left_shift",
                    "lazy": true
                }
            ],
            "to_if_alone": [
                { "key_code": "lang2" }
            ]
        },
        {
            "description": "かな押しっぱなしでshift",
            "type": "basic",
            "from": {
                "key_code": "lang1",
                "modifiers": {
                    "optional": ["any"]
                }
            },
            "to": [
                {
                    "key_code": "left_shift",
                    "lazy": true
                }
            ],
            "to_if_alone": [
                { "key_code": "lang1" }
            ]
        },
        {
            "description": "underscoreをbackslashにする",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "international1",
                        "modifiers": {
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "international3",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "１秒間押し続けないとcommand-qが実行されない設定",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "q",
                        "modifiers": { "mandatory": [ "command" ], "optional": [ "caps_lock" ] }
                    },
                    "parameters": { "basic.to_if_held_down_threshold_milliseconds": 1000 },
                    "to_if_held_down": [
                        { 
                            "key_code": "q",
                            "modifiers": [ "command" ],
                            "repeat": false
                        }
                    ]
                }
            ]
        },
        {
            "description": "日本語入力中はM式配列にする",
